{
  "name": "zan/common-bundle",
  "description": "Common code shared among many Symfony applications",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "doctrine/annotations": "^1.13",
    "doctrine/dbal": ">=2.10.2",
    "symfony/dependency-injection": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Zan\\CommonBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zan\\CommonBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4"
  }
}
